import random  #ya random number  dayta ha
import string  #ya random string dayta ha 


def password_generator(length: int=10): #funcation
   alphabet =string.ascii_letters+string.digits+string.punctuation
   password="".join(random.choice(alphabet) for i in range(length))
   return password
password=password_generator()# calling function
print(F"Your password is: {password}")
